{
  "expo": {
    "name": "Dar-El-Teb",
    "slug": "dar-el-teb",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/images/LOGO-DARELTEB.png",
    "scheme": "darelteb",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,

    "splash": {
      "image": "./assets/images/LOGO-DARELTEB.png",
      "backgroundColor": "#FFFFFF",
      "resizeMode": "contain"
    },

    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "app.darelteb.com",
      "buildNumber": "1.0.1",
      "googleServicesFile": "./GoogleService-Info.plist",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "UIBackgroundModes": ["remote-notification"]
      }
    },

    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/LOGO-DARELTEB.png",
        "backgroundColor": "#E6F4FE"
      },
      "package": "com.darelteb.app",
      "versionCode": 11,
      "googleServicesFile": "./google-services.json",
      "permissions": ["NOTIFICATIONS"],
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },

    "plugins": ["expo-router"],

    "web": {
      "output": "static",
      "favicon": "./assets/images/LOGO-DARELTEB.png"
    },

    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },

    "extra": {
      "router": {},
      "eas": {
        "projectId": "b1f6acf7-dd88-4640-8271-f1028090b7c0"
      }
    }
  }
}