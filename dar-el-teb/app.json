{
  "expo": {
    "name": "Dar-El-Teb",
    "slug": "dar-el-teb",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/images/LOGO-DARELTEB.png",
    "scheme": "darelteb",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/LOGO-DARELTEB.png",
      "backgroundColor": "#FFFFFF",
      "resizeMode": "contain"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "app.darelteb.com",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "UIBackgroundModes": ["remote-notification"] 
      },
      "buildNumber": "1.0.1"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/LOGO-DARELTEB.png",
        "monochromeImage": "./assets/images/LOGO-DARELTEB.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "darelteb.android",
      "versionCode": 11
    },
    "plugins": [
      "expo-router",
      [
        "expo-notifications",
        {
          "icon": "./assets/images/LOGO-DARELTEB.png",
          "color": "#ffffff"
        }
      ]
    ],
    "web": {
      "output": "static",
      "favicon": "./assets/images/LOGO-DARELTEB.png"
    },
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "b1f6acf7-dd88-4640-8271-f1028090b7c0"
      }
    }
  }
}
