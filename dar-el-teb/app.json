{
  "expo": {
    "name": "Dar-El-Teb",
    "slug": "dar-el-teb",
    "version": "1.0.2",
    "orientation": "portrait",
    "icon": "./assets/images/LOGO-DARELTEB.png",
    "scheme": "darelteb",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/LOGO-DARELTEB.png",
      "backgroundColor": "#FFFFFF",
      "resizeMode": "contain"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "app.darelteb.com",
      "buildNumber": "1.0.1",
      "googleServicesFile": "./GoogleService-Info.plist",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "UIBackgroundModes": [
          "remote-notification",
          "remote-notification"
        ],
        "NSUserNotificationsUsageDescription": "نحتاج صلاحيات الإشعارات لإرسال التنبيهات والتحديثات المهمة لك"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/LOGO-DARELTEB.png",
        "backgroundColor": "#E6F4FE"
      },
      "package": "com.dareelteb.app",
      "versionCode": 12,
      "googleServicesFile": "./google-services.json",
      "permissions": [
        "NOTIFICATIONS",
        "FOREGROUND_SERVICE",
        "WAKE_LOCK",
        "CALL_PHONE",
        "INTERNET",
        "NOTIFICATIONS",
        "FOREGROUND_SERVICE",
        "WAKE_LOCK",
        "CALL_PHONE",
        "INTERNET"
      ],
      "intentFilters": [
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "tel"
            },
            {
              "scheme": "https"
            },
            {
              "scheme": "http"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        },
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "tel"
            },
            {
              "scheme": "https"
            },
            {
              "scheme": "http"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ],
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "plugins": [
      "@react-native-firebase/app",
      [
        "@react-native-firebase/messaging",
        {
          "androidNotificationChannelId": "high-priority-channel"
        }
      ],
      "expo-router",
      [
        "expo-build-properties",
        {
          "android": {
            "compileSdkVersion": 36,
            "targetSdkVersion": 36,
            "minSdkVersion": 24,
            "enableProguardInReleaseBuilds": true,
            "extraMavenRepos": [
              "https://maven.google.com"
            ],
            "googleServicesFile": "./google-services.json",
            "enableMinifyInReleaseBuilds": true
          },
          "ios": {
            "useFrameworks": "static",
            "deploymentTarget": "15.1"
          }
        }
      ],
      "expo-web-browser"
    ],
    "web": {
      "output": "static",
      "favicon": "./assets/images/LOGO-DARELTEB.png"
    },
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "330b3e1e-4f74-4527-ade6-fd12591c0841"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "owner": "taher1911",
    "updates": {
      "url": "https://u.expo.dev/330b3e1e-4f74-4527-ade6-fd12591c0841"
    }
  }
}
